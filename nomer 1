#include <iostream>

using namespace std;

int main() {

    int n,i;
    cout << "masukkan niali x: ";
    cin >> n;
    for(i=0; i<=n; i++){
        if(i==0){
            i=i+2;
            cout<<"F (x)= 1 ";
        }
        if(i%2==0){
            cout << "-1/" << i << " ";
        }else{
            cout << "+1/" << i << " ";
        }
    }
return 0;
}
