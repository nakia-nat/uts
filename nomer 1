#include <iostream>

using namespace std;

int main() {

    int n,a;
    cout << "masukkan nilai n: ";
    cin >> n;
    for(a=0; a<=n; a++){
        if(a==0){
            a=a+2;
            cout<<"F (x)= 1 ";
        }
        if(a%2==0){
            cout << "-1/" << a << " ";
        }else{
            cout << "+1/" << a << " ";
        }
    }
return 0;
}
